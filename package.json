{
    "name": "AzuraApp",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "android": "react-native run-android",
        "ios": "react-native run-ios",
        "start": "react-native start",
        "test": "jest",
        "lint": "eslint .",
        "clean": "cd android && ./gradlew clean",
        "assembleDebug": "cd android && ./gradlew assembleDebug",
        "assembleRelease": "cd android && ./gradlew assembleRelease",
        "bundleDebug": "cd android && ./gradlew bundleDebug",
        "bundleRelease": "cd android && ./gradlew bundleRelease",
        "postinstall": "npx jetify",
        "android:build": "fastlane android",
        "ios:beta": "fastlane ios beta",
        "android:beta": "fastlane android beta",
        
        "start:android:dev": "APP_ENV=development ENVFILE=.env.development react-native run-android --variant=DevelopmentDebug",
        "build:android:debug:dev": "cd android && ENVFILE=.env.development ./gradlew assembleDevelopmentDebug",
        "build:android:release:dev": "cd android && ENVFILE=.env.development ./gradlew assembleDevelopmentRelease",
        "start:dev": "APP_ENV=development ENVFILE=.env.development npx react-native start -- --reset-cache",
        "deploy:android:dev": "fastlane android deploy --env development"
    },
    "dependencies": {
        "@react-native-async-storage/async-storage": "^1.14.1",
        "@react-native-community/masked-view": "^0.1.10",
        "@react-native-community/push-notification-ios": "^1.8.0",
        "@react-native-firebase/app": "^11.2.0",
        "@react-native-firebase/messaging": "^11.2.0",
        "@react-native-picker/picker": "^1.14.0",
        "@react-navigation/bottom-tabs": "^5.11.8",
        "@react-navigation/drawer": "^5.12.4",
        "@react-navigation/native": "^5.9.3",
        "@react-navigation/stack": "^5.14.3",
        "apisauce": "^2.1.1",
        "appcenter": "^4.2.0",
        "appcenter-analytics": "^4.2.0",
        "appcenter-crashes": "^4.2.0",
        "code-push-cli": "^3.0.0",
        "lodash": "^4.17.21",
        "native-base": "^2.15.2",
        "react": "16.13.1",
        "react-native": "0.63.0",
        "react-native-animatable": "^1.3.3",
        "react-native-calendar-picker": "^7.1.1",
        "react-native-camera": "^3.43.3",
        "react-native-config": "^1.4.4",
        "react-native-gesture-handler": "^1.10.3",
        "react-native-global-props": "^1.1.5",
        "react-native-image-crop-picker": "^0.36.0",
        "react-native-linear-gradient": "^2.5.6",
        "react-native-push-notification": "^7.2.3",
        "react-native-reanimated": "^2.0.1",
        "react-native-safe-area-context": "^3.2.0",
        "react-native-screens": "^2.18.1",
        "react-native-shadow": "^1.2.2",
        "react-native-svg": "^12.1.0",
        "react-native-vector-icons": "^8.1.0",
        "react-redux": "^7.2.3",
        "redux": "^4.0.5",
        "redux-logger": "^3.0.6",
        "redux-thunk": "^2.3.0"
    },
    "devDependencies": {
        "@babel/core": "^7.13.10",
        "@babel/runtime": "^7.13.10",
        "@react-native-community/eslint-config": "^2.0.0",
        "babel-jest": "^26.6.3",
        "eslint": "^7.22.0",
        "jest": "^26.6.3",
        "metro-react-native-babel-preset": "^0.65.2",
        "react-test-renderer": "16.13.1",
        "standard": "^16.0.3"
    },
    "jest": {
        "preset": "react-native"
    },
    "standard": {
        "parser": "@babel/eslint-parser",
        "globals": [
            "describe",
            "test",
            "jest",
            "expect",
            "fetch",
            "navigator",
            "__DEV__",
            "XMLHttpRequest",
            "FormData",
            "React$Element",
            "UNSAFE_componentWillReceiveProps",
            "UNSAFE_componentWillMount"
        ]
    }
}